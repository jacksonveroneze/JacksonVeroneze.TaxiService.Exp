{
  "DistributedTracing": {
    "IsEnabled": true,
    "Jaeger": {
      "Host": "172.17.0.1",
      "Port": 6831
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Seq"
    ],
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console",
              "Args": {
                "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Literate, Serilog.Sinks.Console",
                "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext}{NewLine}{Message:lj} {Properties:j}{NewLine}{Exception}{NewLine}"
              }
            },
            {
              "Name": "Seq",
              "Args": {
                "serverUrl": "http://172.17.0.1:5341"
              }
            }
          ]
        }
      }
    ]
  },
  "CacheType": "Redis",
  "CacheEndpoint": "127.0.0.1:6379",
  "City": {
    "CacheExpMilisegundos": "10000"
  },
  "State": {
    "CacheExpMilisegundos": "10000"
  },
  "HttpClients": [
    {
      "Name": "ibge",
      "BaseAddress": "http://servicodados.ibge.gov.br"
    }
  ]
}
